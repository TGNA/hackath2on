<% content_for :head do %>
  <link rel="apple-touch-icon-precomposed" sizes="144x144" href="img/touch/apple-touch-icon-144x144-precomposed.png">
  <link rel="apple-touch-icon-precomposed" sizes="114x114" href="img/touch/apple-touch-icon-114x114-precomposed.png">
  <link rel="apple-touch-icon-precomposed" sizes="72x72" href="img/touch/apple-touch-icon-72x72-precomposed.png">
  <link rel="apple-touch-icon-precomposed" href="img/touch/apple-touch-icon-57x57-precomposed.png">
  <link rel="shortcut icon" sizes="196x196" href="img/touch/touch-icon-196x196.png">
  <link rel="shortcut icon" href="img/touch/apple-touch-icon.png">

  <!-- Tile icon for Win8 (144x144 + tile color) -->
  <meta name="msapplication-TileImage" content="img/touch/apple-touch-icon-144x144-precomposed.png">
  <meta name="msapplication-TileColor" content="#222222">
  <meta name="mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black">
  <meta name="apple-mobile-web-app-title" content="">
  <script>(function(a,b,c){if(c in b&&b[c]){var d,e=a.location,f=/^(a|html)$/i;a.addEventListener("click",function(a){d=a.target;while(!f.test(d.nodeName))d=d.parentNode;"href"in d&&(d.href.indexOf("http")||~d.href.indexOf(e.host))&&(a.preventDefault(),e.href=d.href)},!1)}})(document,window.navigator,"standalone")</script>
  <%= stylesheet_link_tag "mobile" %>
  <%= javascript_include_tag 'mobile' %>
<% end %>
<nav>
  
</nav>
<ul class="mobile-content">
  <li data-content="home" class="selected">
    <div class="widget-container">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title">Water consumption - Home<i class="fa fa-eye close"></i></h3>
        </div>
        <div class="inner-widget-container home-panel">
          <%= render partial: "dashboard/tables/homes" %>
        </div>
      </div>
    </div>
  </li>
  <li data-content="savings">            
    <div class="widget-container">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title">My savings<i class="fa fa-eye close"></i></h3>
        </div>
        <div class="inner-widget-container">
          <%= render partial: "dashboard/tables/last_one" %>
        </div> 
      </div>
    </div>
  </li>
  <li data-content="versus">            
    <div class="widget-container">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title">My Home versus System<i class="fa fa-eye close"></i></h3>
        </div>
        <div class="inner-widget-container reward">
          <%= render partial: "dashboard/tables/reward" %>
        </div>
      </div>
    </div>
  </li>
  <li data-content="efficiency">            
    <div class="widget-container">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title">System efficiency<i class="fa fa-eye close"></i></h3>
        </div>
        <div class="inner-widget-container total-saving">
          <%= render partial: "dashboard/tables/week" %>
        </div>
      </div>
    </div>
  </li>
  <li data-content="neighborhood">            
    <div class="widget-container">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title">Water consumption by neighborhood<i class="fa fa-eye close"></i></h3>
        </div>
        <div class="inner-widget-container">
          <%= render partial: "dashboard/tables/total_saving" %>
        </div>
      </div>
    </div>
  </li>
  <li data-content="consumption">            
    <div class="widget-container">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title">Water consumption - System<i class="fa fa-eye close"></i></h3>
        </div>
        <div class="inner-widget-container">
          <%= render partial: "dashboard/tables/system" %>
        </div>
      </div>
    </div>
  </li>
</ul>
<footer class="tabbar">
  <ul>
    <li class="active">
      <a href="#" class="selected" data-content="home"><i class="fa fa-tachometer fa-2x"></i>
        <p class="text-center">Hola</p>
      </a>
    </li>
    <li class="">
      <a href="#" data-content="savings"><i class="fa fa-envelope-o fa-2x"></i>
        <p class="text-center">Water Usage</p>
      </a>
    </li>
    <li class="">
      <a href="#" data-content="versus"><i class="fa fa-calendar fa-2x"></i>
        <p class="text-center">Usage of Water compared by Location</p>
      </a>
    </li>
    <li class="">
      <a href="#" data-content="efficiency"><i class="fa fa-wrench fa-2x"></i>
        <p class="text-center">Total Savings</p>
      </a>
    </li>
    <li class="">
      <a href="#" data-content="neighborhood"><i class="fa fa-wrench fa-2x"></i>
        <p class="text-center">Hola</p>
      </a>
    </li>
    <li class="">
      <a href="#"  data-content="consumption"><i class="fa fa-wrench fa-2x"></i>
        <p class="text-center">Hola</p>
      </a>
    </li>
  </ul>
</footer>